//change redcolor background desktop    if not found  do nothing

changedesktopcolor_filepath = project_path & '''\cpp_commands\change_color_desktop_to_red.exe'''

exist_bRet = File.FileExists(changedesktopcolor_filepath)
TracePrint(exist_bRet)

If Trim(CStr(changedesktopcolor_filepath)) <> ""  AND exist_bRet = true
    //change redbgcolor
    Dim iPIDredbgcolor
    iPIDredbgcolor = App.Run(changedesktopcolor_filepath, 0, 1)

    Delay(100)

End If


